<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tescomm.tescommlib.MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/bt_photohelper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="photohelper Demo"
            android:onClick="photohelperClick"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="16dp"/>
        <Button
            android:id="@+id/bt_addresspicker"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="addresspicker Demo"
            android:onClick="addresspickerClick"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="16dp"/>

    </LinearLayout>


</android.support.constraint.ConstraintLayout>
